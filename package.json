{
  "name": "jsreport-version-control",
  "version": "0.2.0",
  "description": "jsreport extension adding support for versioning templates and other entities",
  "main": "index.js",
  "scripts": {
    "test": "set DEBUG=jsreport && mocha test && standard",
    "start": "jsreport-studio-start --httpPort 5488",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "homepage": "https://github.com/jsreport/jsreport-version-control",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-version-control.git"
  },
  "license": "MIT",
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-standard": "3.0.1",
    "in-publish": "2.0.0",
    "jsreport-assets": "github:jsreport/jsreport-assets#2.0.0-beta",
    "jsreport-authentication": "github:jsreport/jsreport-authentication#2.0.0-beta",
    "jsreport-chrome-pdf": "github:jsreport/jsreport-chrome-pdf#2.0.0-beta",
    "jsreport-core": "github:jsreport/jsreport-core#2.0.0-beta",
    "jsreport-data": "github:jsreport/jsreport-data#2.0.0-beta",
    "jsreport-express": "github:jsreport/jsreport-express#2.0.0-beta",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "github:jsreport/jsreport-templates#2.0.0-beta",
    "mocha": "5.1.1",
    "puppeteer": "1.2.0",
    "rimraf": "2.6.2",
    "should": "13.2.1",
    "standard": "11.0.1",
    "supertest": "3.0.0"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "diff": "3.4.0",
    "diff2html": "2.3.2",
    "node.extend": "2.0.0"
  },
  "files": [
    "lib",
    "studio",
    "test",
    "static",
    "index.js",
    "jsreport.config.js"
  ]
}
